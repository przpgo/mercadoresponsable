            "& when (@showText)": {
              "p.main-text": {
                "margin-bottom": "2.6875rem"
              }
            },
            "& when (@showAccordion)": {
              ".amp-accordion": {
                "margin-bottom": "2.875rem"
              },
              ".accordion-title": {
                "padding": "1.3rem 2rem",
                "&:focus": {
                  "outline": "none"
                }
              },
              ".accordion-text": {
                "margin": "0",
                "padding": "1.3rem 2rem"
              },
              "& when (@transparentBg)": {
                ".accordion-title, .accordion-item-body": {
                  "background": "transparent"
                }
              },
              "& when not (@transparentBg)": {
                ".accordion-title": {
                  "background": "@headBg"
                },
                ".accordion-item-body": {
                  "background": "@bodyBg"
                }
              },
              ".accordion-item:not(.last-child)": {
                "border-bottom": "1px solid @borderColor"
